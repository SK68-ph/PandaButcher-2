<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true" indentsize="4">
			<listeners>
				<add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
				<add name="textWriterListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="ErrorLogs.txt"/>
				<add name="eventLogListener" type="System.Diagnostics.EventLogTraceListener" initializeData="EventLog" />
				<remove name="Default"/>
			</listeners>
		</trace>
	</system.diagnostics>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>